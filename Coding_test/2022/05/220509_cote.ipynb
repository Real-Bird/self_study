    "n, m = map(int, input().split())\n",
    "n1 = list(map(int, input().split()))\n",
    "prefix_sum = [0]\n",
    "\n",
    "temp = 0\n",
    "for i in n1:\n",
    "    temp += i\n",
    "    prefix_sum.append(temp)\n",
    "\n",
    "for _ in range(m):\n",
    "    i, j = map(int, input().split())\n",
    "    print(prefix_sum[j] - prefix_sum[i-1])\n"
